<!--components/w-swiper/w-swiper.wxml-->
<!-- <text>components/w-swiper/w-swiper.wxml</text> -->
<swiper class="swiper"
        circular="true"
        autoplay="true"
        interval='3000'
        duration='300'
        indicator-dots="true"
        indicator-active-color='#ff5777'>
    <block wx:for="{{list}}" wx:key="index">
        <swiper-item class="swiper-item" >
            <image src="{{item.image}}" mode="widthFix" class="swiper-item-img" />
        </swiper-item>
    </block>
</swiper>
